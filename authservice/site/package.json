{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4",
    "proxy": "http://localhost:5001/"
  },
  "scripts": {
    "start": "react-scripts start --no-browser && yarn linkpro",
    "build": "react-scripts build && yarn linkpro",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "linkpro": "yarn linkrem && yarn linkcopy",
    "linkrem": "rm -fr ../templates/index.html ../static",
    "linkcopy": "cp -uf ./build/index.html ../templates/ && cp -f -r ./build/static ../ && cp -uf ./build/favicon.ico ./build/asset-manifest.json ./build/manifest.json ./build/robots.txt ../static "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
